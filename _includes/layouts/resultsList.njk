---
layout: layouts/base.njk
templateClass: tmpl-page

---
<section>

<h1 class="post-title">{{ title }}</h1>

{{ content | safe }}

<ul class="result-list">
  {% for item in results[list] %}
    <li class="media-box">
      <a href="{{ item.team | slug }}">
        {{ item.team }}
        </a> {% if item.topTen %}<i class='award' title="Top 10">⭐</i>{% endif %}
        {% if item.award == "Grand Prize" %}<i class='award'>🥇</i>
        {% elif item.award  %}<i class='award'>🥈</i>{% endif %}

      </li>
    {% endfor %}
  </ul>
<hr>

</section>
